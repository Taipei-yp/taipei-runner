{
  "name": "taipei-runner",
  "version": "0.0.1",
  "description": "Simple runner game",
  "author": "Taipei",
  "license": "ISC",
  "keywords": [
    "runner",
    "game",
    "yandex.praktikum"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taipei-yp/taipei-runner.git"
  },
  "bugs": {
    "url": "https://github.com/Taipei-yp/taipei-runner/issues"
  },
  "homepage": "https://github.com/Taipei-yp/taipei-runner#readme",
  "main": "client/index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "build": "run-s build-prod",
    "build-dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js",
    "build-prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "test": "jest",
    "lint": "run-p lint:js lint:css lint:pretty",
    "lint-write": "run-s lint:pretty-write lint:js-write",
    "lint:js": "eslint ./client --ext .ts,.tsx --cache",
    "lint:js-write": "eslint ./client --ext .ts,.tsx --cache --fix",
    "lint:css": "stylelint ./client/**/*.css",
    "lint:pretty": "prettier --check client/**/*.{ts,tsx,css}",
    "lint:pretty-write": "prettier --write client/**/*.{ts,tsx,css}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.(ts|tsx|css|scss)": [
      "npm run lint:pretty-write",
      "npm run lint:js-write",
      "npm run lint:css"
    ]
  },
  "dependencies": {
    "axios": "0.21.1",
    "bem-cn": "3.0.1",
    "core-js": "3.8.1",
    "final-form": "4.20.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
    "react-hot-loader": "4.13.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/preset-env": "7.12.10",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@hot-loader/react-dom": "17.0.1",
    "@testing-library/dom": "7.29.0",
    "@testing-library/jest-dom": "5.11.6",
    "@types/jest": "26.0.19",
    "@types/jest-in-case": "1.0.3",
    "@types/node": "14.14.12",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.12",
    "@types/react-router-dom": "5.1.6",
    "@types/react-test-renderer": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "autoprefixer": "10.1.0",
    "babel-loader": "8.2.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "clean-webpack-plugin": "3.0.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.1.5",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "express": "4.17.1",
    "fork-ts-checker-notifier-webpack-plugin": "3.0.0",
    "fork-ts-checker-webpack-plugin": "6.0.6",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.5",
    "identity-obj-proxy": "3.0.0",
    "import-glob-loader": "1.1.0",
    "jest": "26.6.3",
