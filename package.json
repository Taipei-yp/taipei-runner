    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "typescript": "4.1.3",
    "webpack": "5.10.1",
