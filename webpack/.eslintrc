{
  "root": true,
  "env": {
    "node": true,
    "es2017": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb/base",
    "eslint:recommended",
    "plugin:eslint-comments/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved":"off",
    "prettier/prettier": ["error",{"endOfLine":"auto"}]
    
  }
}
